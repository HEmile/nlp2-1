@inproceedings{Riley+2012:VBWA,
	Address = {Jeju Island, Korea},
	Author = {Riley, Darcey and Gildea, Daniel},
	Booktitle = {Proceedings of the 50th Annual Meeting of the Association for Computational Linguistics (Volume 2: Short Papers)},
	Date-Added = {2015-03-05 10:47:37 +0000},
	Date-Modified = {2015-03-12 13:04:12 +0000},
	Keywords = {MT, WA},
	Month = {July},
	Pages = {306--310},
	Publisher = {Association for Computational Linguistics},
	Title = {Improving the IBM Alignment Models Using Variational Bayes},
	Url = {http://www.aclweb.org/anthology/P12-2060},
	Year = {2012},
	Annote = {This work adresses the known issue of MLE overfitting by applying variational Bayes (VB).
VB modifies the M-step of EM by applying a function over expected counts. This function can be seen as a prior over the expected counts (note how this leads to far less hyperparameters than directly modelling a prior over the parameters). In a way, VB can be seen as a form of smoothing of the expected counts controlled by the hyperparameters of the prior (thus anti-smoothing can also be obtained).
In this paper, authors employ a Dirichlet prior.

They show interesting gains wrt AER, overfitting and BLEU when applying VB to IBM model 1 with low alpha (which is a form of anti-smoothing), where one is basically addressing to problem of over condifent alignments for rare words (the garbage collector phenomenon discussed by Moore (2004)).},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.aclweb.org/anthology/P12-2060}}

