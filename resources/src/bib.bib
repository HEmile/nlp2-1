@incollection{Chappelier+2000:MCParsing,
	Author = {Chappelier, Jean-C{\'e}dric and Rajman, Martin},
	Booktitle = {Natural Language Processing --- NLP 2000},
	Date-Added = {2015-03-02 08:53:51 +0000},
